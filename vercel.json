{
  "version": 2,
  "functions": {
    "app.py": {
      "runtime": "python3.9"
    },
    "api/webhook.py": {
      "runtime": "python3.9"
    }
  },
  "routes": [
    {
      "src": "/api/webhook",
      "dest": "/api/webhook.py"
    },
    {
      "src": "/(.*)",
      "dest": "/app.py"
    }
  ],
  "env": {
    "BOT_TOKEN": "@bot_token",
    "CHANNEL_ID": "@channel_id",
    "WEBAPP_BASE_URL": "@webapp_base_url"
  }
}