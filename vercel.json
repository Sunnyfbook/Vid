{
  "version": 2,
  "functions": {
    "api/webhook.py": 
      "maxDuration": 30
    },
    "index.py": {
      "maxDuration": 30
    }
  },
  "routes": [
    {
      "src": "/api/webhook",
      "dest": "/api/webhook.py",
      "methods": ["GET", "POST"]
    },
    {
      "src": "/api/video/(.*)",
      "dest": "/api/webhook.py"
    },
    {
      "src": "/api/videos",
      "dest": "/api/webhook.py"
    },
    {
      "src": "/(.*)",
      "dest": "/index.py"
    }
  ]
}
